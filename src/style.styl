/******FadingTitle******/
h1 {
  font-size: 10vw;
  top: -35vw; text-shadow: 0 0 3px 4px rgba(0,0,0,0.3);
  opacity: 0;
  animation: fade 4s 2s forwards; 
}
h2 {
  font-weight: 100;
  font-size: 3vw; 
  top: 20vw;
  letter-spacing: .2vw;
  opacity: 0;
}
h2:hover { animation: fade 1s 2s forwards; }

#intro {
  background: linear-gradient(#000,#0001),rgba(255,255,255,0.9);
  background-size: 100% 100%, 100% 100%;
  //padding-top: 56.44%;
  background-position: 0 -99vh, 0 0;
  background-repeat: no-repeat;
  background-blend-mode: darken;//soft-light;//luminosity
  -webkit-background-blend-mode: darken;//safari&webkit
  transition: 1s background-position ease-in-out;//linear
  margin: 0 auto;
  width: 100wh;
  height: 100vh;
  position: initial;//absolute
  opacity: 0.9;
}
#intro:hover {background-position: 0 0, 0 0; }

#intro h1 {
  text-align: left;
  margin: 0px;//10px
  padding-top: 10px;
  padding-left: 10px;
  color: #000;
  font-family: inherit;
  font-size: 5vw;
}
#intro p{
  text-align: left;
  width: 80%;
  margin: 0 auto;
  color: #fff;
  font-family: inherit;
  margin-bottom: 5vh;
  font-size: 2vw;
}
#intro a {
  color: #fff;
}
#intro input {
  display: block;
  position: absolute;
  top: 0;//95vh;
  left: 0;
  right: 0;
  margin: 0 auto;
  border: 0px solid #00ff00;
  border-radius:2px;
  color: rgba(255,255,255,0.2);//text
  background: rgba(0,0,0,0.0);//background
  transition: opacity 0.5s;
-webkit-transition: all ease 0.5s;
-moz-transition: all ease 0.5s;
-o-transition: all ease 0.5s;
-ms-transition: all ease 0.5s ;
transition: all ease 0.5s ;
}
#intro input:active {
  border: 2px solid rgba(255,0,0,0.8);
  color: rgba(255,0,0,1);//text color RED
  background: rgba(255,255,255,0.1);//background RED
}
#intro input:hover {
background:rgba(255,0,0,0.8);
color:#fff;
-webkit-transition: all ease 0.5s;
-moz-transition: all ease 0.5s;
-o-transition: all ease 0.5s;
-ms-transition: all ease 0.5s ;
transition: all ease 0.5s ;
} 
#bottomButton {
  position: absolute;
  //top: //6950x;
  bottom: 0;//pied de page
  left: 50%;
  //margin: 0 auto;
  border: 0px solid #00ff00;
  border-radius:2px;
  color: rgba(255,255,255,0.2);//text
  background: rgba(0,0,0,0.0);//background
  transition: opacity 0.5s;
-webkit-transition: all ease 0.5s;
-moz-transition: all ease 0.5s;
-o-transition: all ease 0.5s;
-ms-transition: all ease 0.5s ;
transition: all ease 0.5s ;
}
#intro input:active {
  border: 2px solid rgba(255,0,0,0.8);
  color: rgba(255,0,0,1);//text color RED
  background: rgba(255,255,255,0.1);//background RED
}
#intro input:hover {
background:rgba(255,0,0,0.8);
color:#fff;
-webkit-transition: all ease 0.5s;
