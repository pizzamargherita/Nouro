#clock-container { 
  display: block;
  position: relative;
  width: 20vw;
//  padding-bottom: 100%;
//  vertical-align: middle;
  overflow: hidden;
  margin-top: 800px;//% vh
  left: 1vw;
//  background: midnightblue;
} 
#face { stroke-width: 2px; stroke: #fff; }
#hour, #min, #sec { 
  stroke-width: 1px;
  fill: #333;
  stroke: #555;
}
#sec { stroke: #f55; }
  
/**********Pendulum*************/
@keyframes sway { 
	to { transform: rotate(30deg);   } 
}
@keyframes fade {
	to { opacity: 1; }
}
@keyframes widen {
	to { width: 40%; }
}
@keyframes secondcount {
	to { transform: rotate(355deg); }
}
#watch { 
  width: 12vw; height: auto;
  margin: 0 auto;
  transform-origin: center top;
  transform: rotate(-30deg); 
  top: -10px; 
  margin-top: 800px;//% vh
  animation: sway 2.2s infinite alternate ease-in-out;
} 
#watch img.watch { width: 100%; height: auto; }
img.seconds { 
  position: absolute;
  width: 8%; height:auto;
  bottom: 11.75%; left: 45%;
  transform-origin: center 72.4%;
  animation: secondcount 60s infinite linear; 
}
/**********PolaEffect**********/
#pola { 
  position: relative; //absolute
  margin-top: 800px;//% vh
  left: 60vw;
  margin: auto 1.2rem;
  float: left; border: 1.25vw solid #e3e4d3;//25px
  border-bottom-width: 2.5vw; //50px
	-webkit-transition: 3s box-shadow ease-in; 
  -moz-transition: 3s box-shadow ease-in; 
  -o-transition: 3s box-shadow ease-in; 
  transition: 3s box-shadow ease-in; 
	box-shadow: 0 0 10vw 10vw rgba(29,25,4,1) inset, 0 0 0.15vw 0.3vw rgba(0,0,0,0.07);//200px 200px, 0 0 3px 6px
  width: 20vw; height: 18.2vw;//400px 364px
  background-position: center;//to center background photo
  background-image:url(http://demosthenes.info/assets/images/christine-adams.jpg);
  background-size: 20vw 20vw;
  background-repeat: no-repeat;
  -webkit-filter: sepia(50%); 
  -o-filter: sepia(50%); 
  -ms-filter: sepia(50%); 
  -moz-filter: sepia(50%); 
  filter: sepia(50%); 
}
#pola:hover { 
  box-shadow: 0 0 100px 0 rgba(29,25,4,0.8) inset, 0 0 3px 6px rgba(0,0,0,0.07); 
  -webkit-filter: sepia(20%); 
  -o-filter: sepia(20%); 
  -ms-filter: sepia(20%); 
  -moz-filter: sepia(20%); 
  filter: sepia(20%);
}

